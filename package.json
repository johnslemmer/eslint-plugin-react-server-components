{
  "name": "@johnslemmer/eslint-plugin-react-server-components",
  "version": "2.0.0",
  "description": "Rules for React server components",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "jest",
    "build": "tsup",
    "lint": "tsc",
    "version": "changeset version && pnpm install --lockfile-only",
    "release": "pnpm build && changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnslemmer/eslint-plugin-react-server-components.git"
  },
  "keywords": [
    "eslint-plugin",
    "eslint",
    "react",
    "next.js"
  ],
  "author": "John Slemmer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johnslemmer/eslint-plugin-react-server-components/issues"
  },
  "homepage": "https://github.com/johnslemmer/eslint-plugin-react-server-components#readme",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/eslint": "^9.0.0",
    "@types/estree": "^1.0.8",
    "@types/estree-jsx": "^1.0.0",
    "@types/jest": "^29.5.2",
    "eslint": "^9.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.1.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "packageManager": "pnpm@9.1.2",
  "dependencies": {
    "eslint-plugin-react": "^7.37.0",
    "globals": "^13.20.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
